{
  "name": "client-pc-app",
  "version": "1.0.0",
  "description": "Client for Price Comparison app",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/helpers/setup.js --colors",
    "lint": "standard",
    "build": "webpack",
    "watch": "webpack --watch",
    "watch-client": "better-npm-run watch-client",
    "start-dev": "better-npm-run start-dev",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html --require babel-register --extension .jsx npm test",
    "dev": "concurrent \"npm run watch-client\" \"npm run start-dev\""
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./js"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./js",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "axios": "0.14.0",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "^6.16.0",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.3.19",
    "file-loader": "^0.9.0",
    "hoist-non-react-statics": "^1.2.0",
    "invariant": "^2.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "map-props": "^1.0.0",
    "material-ui": "^0.16.1",
    "multireducer": "^3.0.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.6.0",
    "react-form-data": "^0.2.3",
    "react-helmet": "^3.1.0",
    "react-inline-css": "^2.3.0",
